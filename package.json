{
  "name": "ng2-prism",
  "version": "2.1.7",
  "description": "An example repository for building your own Angular 2 library",
  "main": "codeblock.js",
  "scripts": {
    "make": "node tasks/make.js",
    "watch": "tsc -w",
    "build": "npm run import-langs && npm run build-directives && rm -rf bundle && tsc && npm run build-css && node tasks/make.js && npm run bundle-langs",
    "build-directives": "node tasks/buildLanguageDirectives.js",
    "import-langs": "node tasks/importPrismLanguages.js",
    "bundle-langs": "node tasks/bundleCompiledLanguages.js",
    "build-css": "node tasks/buildCSS.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tpadjen/ng2-prism.git"
  },
  "keywords": [
    "Angular",
    "2",
    "Prism"
  ],
  "author": "Tim Padjen",
  "license": "MIT",
  "typings": "./codeblock.d.ts",
  "bugs": {
    "url": "https://github.com/tpadjen/ng2-prism/issues"
  },
  "homepage": "https://github.com/tpadjen/ng2-prism#readme",
  "devDependencies": {
    "angular2": "2.0.0-beta.7",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "node-sass": "^3.4.2",
    "prismjs": "git://github.com/PrismJS/prism.git#gh-pages",
    "reflect-metadata": "0.1.2",
    "rimraf": "^2.5.2",
    "rxjs": "5.0.0-beta.2",
    "systemjs-builder": "^0.15.7",
    "typescript": "1.7.5",
    "zone.js": "0.5.15"
  },
  "peerDependencies": {
    "angular2": "2.0.0-beta.7",
    "es6-promise": "3.0.2",
    "es6-shim": "^0.33.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "0.5.15"
  }
}
